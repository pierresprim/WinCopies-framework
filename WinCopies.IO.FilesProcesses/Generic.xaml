<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WinCopies.IO.FileProcesses"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
             x:Class="WinCopies.IO.FileProcesses.Generic">
    <sys:String x:Key="IncompatibleValues" >{0} cannot be set to true when {1} has the None value.</sys:String>
    <sys:String x:Key="LoadFilesInfoModuleHasNotRanYet" >The Loading files info module has not loaded. You must load it first.</sys:String>
    <sys:String x:Key="LoadFilesInfoModuleIsRunning" >The LoadingFilesInfo module is running.</sys:String>
</ResourceDictionary>
<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<PackageId>WinCopies.Reflection.DotNetParser</PackageId>
		<RootNamespace>WinCopies.Reflection.DotNetParser</RootNamespace>
		<TargetFrameworks>netcoreapp3.0;netcoreapp3.1;net5.0-windows;net6.0-windows;net7.0-windows;net8.0-windows;net472;net48</TargetFrameworks>
		<Copyright>Copyright © 2022 Pierre Sprimont</Copyright>
		<VersionPrefix>3.15</VersionPrefix>
		<VersionSuffix>preview</VersionSuffix>
		<AssemblyVersion>3.15</AssemblyVersion>
		<FileVersion>3.15</FileVersion>
	</PropertyGroup>

	<Import Project="..\..\..\Shared\Reflection\WinCopies.Reflection.DotNetParser.Shared\WinCopies.Reflection.DotNetParser.Shared.projitems" Label="Shared" />

	<Import Project="$(SolutionDir)\ExtraProperties.props" Condition="'$(Configuration)'=='Release'" />

	<ItemGroup>
		<PackageReference Include="WinCopies.Collections" Version="3.21" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\..\..\LICENSE.md">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\..\..\WinCopies.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <None Update="README.md">
	    <PackagePath></PackagePath>
	    <Pack>true</Pack>
	  </None>
	</ItemGroup>

</Project>

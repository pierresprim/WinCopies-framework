<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<PackageId>WinCopies.GUI.Models</PackageId>
		<AssemblyName>WinCopies.GUI.Models</AssemblyName>
		<RootNamespace>WinCopies.GUI.Models</RootNamespace>
		<Description>Models for the WinCopies framework.</Description>
		<TargetFrameworks>netcoreapp3.0;netcoreapp3.1;net5.0-windows;net6.0-windows;net7.0-windows;net472;net48</TargetFrameworks>
		<UseWPF>true</UseWPF>
		<Copyright>Copyright Â© 2021 Pierre Sprimont</Copyright>
		<PackageTags>WinCopies, GUI, WPF, Models</PackageTags>
		<DocumentationFile>WinCopies.GUI.Models.xml</DocumentationFile>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>WinCopies.GUI.Models.snk</AssemblyOriginatorKeyFile>
		<DefineConstants>WinCopies2;WinCopies3;$(DefineConstants)</DefineConstants>
		<VersionPrefix>3.13</VersionPrefix>
		<VersionSuffix>preview</VersionSuffix>
		<AssemblyVersion>3.13</AssemblyVersion>
		<FileVersion>3.13</FileVersion>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="WinCopies.Util" Version="3.17-preview" />
		<PackageReference Include="WinCopies.Util.Desktop" Version="3.17-preview" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\..\..\LICENSE.md">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\..\..\WinCopies.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<Import Project="..\..\Shared\WinCopies.GUI.Models.Shared\WinCopies.GUI.Models.Shared.projitems" Label="Shared" />

</Project>

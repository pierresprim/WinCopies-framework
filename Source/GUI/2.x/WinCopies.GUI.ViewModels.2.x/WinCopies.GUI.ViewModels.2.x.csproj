<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <PackageId>WinCopies.GUI.ViewModels</PackageId>
    <AssemblyName>WinCopies.GUI.ViewModels</AssemblyName>
    <RootNamespace>WinCopies.GUI.ViewModels</RootNamespace>
    <TargetFrameworks>netcoreapp3.0;netcoreapp3.1;net5.0-windows;net472;net48</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <Description>View models for the WinCopies framework.</Description>
    <Copyright>Copyright Â© 2020 Pierre Sprimont</Copyright>
    <PackageTags>WinCopies, GUI, WPF, ViewModels</PackageTags>
    <DocumentationFile>WinCopies.GUI.ViewModels.xml</DocumentationFile>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>WinCopies.GUI.ViewModels.snk</AssemblyOriginatorKeyFile>
    <DefineConstants>WinCopies2;$(DefineConstants)</DefineConstants>
    <VersionPrefix>2.5.10</VersionPrefix>
    <VersionSuffix>preview</VersionSuffix>
    <AssemblyVersion>2.5.10</AssemblyVersion>
    <FileVersion>2.5.10</FileVersion>
  </PropertyGroup>

  <Import Project="..\..\WinCopies.GUI.ViewModels.Shared\WinCopies.GUI.ViewModels.Shared.projitems" Label="Shared" />

  <ItemGroup>
    <ProjectReference Include="..\WinCopies.GUI.Models.2.x\WinCopies.GUI.Models.2.x.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>

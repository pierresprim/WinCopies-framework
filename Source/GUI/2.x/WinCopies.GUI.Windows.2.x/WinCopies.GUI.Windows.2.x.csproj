<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <PackageId>WinCopies.GUI.Windows</PackageId>
    <AssemblyName>WinCopies.GUI.Windows</AssemblyName>
    <RootNamespace>WinCopies.GUI.Windows</RootNamespace>
    <TargetFrameworks>netcoreapp3.0;netcoreapp3.1;net5.0-windows;net472;net48</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>WinCopies.GUI.Windows.snk</AssemblyOriginatorKeyFile>
    <DocumentationFile>WinCopies.GUI.Windows.xml</DocumentationFile>
    <DefineConstants>WinCopies2;$(DefineConstants)</DefineConstants>
    <VersionPrefix>2.5.10</VersionPrefix>
    <VersionSuffix>preview</VersionSuffix>
    <AssemblyVersion>2.5.10</AssemblyVersion>
    <FileVersion>2.5.10</FileVersion>
  </PropertyGroup>

  <Import Project="..\..\WinCopies.GUI.Windows.Shared\WinCopies.GUI.Windows.Shared.projitems" Label="Shared" />

  <ItemGroup>
    <PackageReference Include="WinCopies.WindowsAPICodePack.Shell" Version="2.1.0" />
    <PackageReference Include="WinCopies.Util.Desktop" Version="2.6.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\WinCopies.GUI.2.x\WinCopies.GUI.2.x.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>

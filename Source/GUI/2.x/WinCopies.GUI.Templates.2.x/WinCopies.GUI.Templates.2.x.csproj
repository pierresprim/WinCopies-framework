<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <PackageId>WinCopies.GUI.Templates</PackageId>
    <AssemblyName>WinCopies.GUI.Templates</AssemblyName>
    <RootNamespace>WinCopies.GUI.Templates</RootNamespace>
    <TargetFrameworks>netcoreapp3.0;netcoreapp3.1;net5.0-windows;net472;net48</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <Description>Templates for the WinCopies framework.</Description>
    <Copyright>Copyright Â© 2020 Pierre Sprimont</Copyright>
    <PackageTags>WinCopies, GUI, WPF, Templates</PackageTags>
    <DocumentationFile>WinCopies.GUI.Templates.xml</DocumentationFile>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>WinCopies.GUI.Templates.snk</AssemblyOriginatorKeyFile>
    <DefineConstants>WinCopies2;$(DefineConstants)</DefineConstants>
    <VersionPrefix>2.5.10</VersionPrefix>
    <VersionSuffix>preview</VersionSuffix>
    <AssemblyVersion>2.5.10</AssemblyVersion>
    <FileVersion>2.5.10</FileVersion>
  </PropertyGroup>

  <Import Project="..\..\WinCopies.GUI.Templates.Shared\WinCopies.GUI.Templates.Shared.projitems" Label="Shared" />

  <ItemGroup>
    <PackageReference Include="WinCopies.Util.Desktop" Version="2.6.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\WinCopies.Util.Temp.2.x\WinCopies.Util.Temp.2.x.csproj" />
    <ProjectReference Include="..\WinCopies.GUI.Models.2.x\WinCopies.GUI.Models.2.x.csproj" />
    <ProjectReference Include="..\WinCopies.GUI.ViewModels.2.x\WinCopies.GUI.ViewModels.2.x.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Themes\Generic.xaml">
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
